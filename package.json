{
  "name": "svggeom",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "test": "c8 npx tap --no-coverage --reporter=terse 'test/*.tap.*js'",
    "c8rephtml": "c8 report -r html",
    "c8report": "c8 report -r text-lcov > coverage/lcov.info",
